class Calculator
{
   public static void main(String[] args) 
   {
      int operand1 = 10;
      int operand2 = 5;
      int result;
      
      result = operand1 + operand2;
      System.out.println(operand1 + " + " + operand2 + " = " + result);

      result = operand1 - operand2;
      System.out.println(operand1 + " - " + operand2 + " = " + result);

      result = operand1 * operand2;
      System.out.println(operand1 + " * " + operand2 + " = " + result);

      result = operand1 / operand2;
      System.out.println(operand1 + " / " + operand2 + " = " + result);

      result = operand1 % operand2;
      System.out.println(operand1 + " % " + operand2 + " = " + result);
   }
}  
