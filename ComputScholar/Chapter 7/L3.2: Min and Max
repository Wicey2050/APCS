import java.util.*;
class Calculator
{
   public static void main(String[] args)
   {
       Scanner input = new Scanner(System.in);
       System.out.print("Enter two integer values: ");
       int value1 = input.nextInt();
       int value2 = input.nextInt();
       input.close();
       
       // student code here
        System.out.println("Largest value = " + Math.max(value1, value2));
        System.out.println("Smallest value = " + Math.min(value1, value2));

   }
} 
                
