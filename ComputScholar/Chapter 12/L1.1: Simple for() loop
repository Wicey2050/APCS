import java.util.*;
class Loopy
{
   public static void main(String[] args)
   {
       Scanner input = new Scanner(System.in);
       System.out.print("How many columns? ");
       int numColumns = input.nextInt();
       input.close();
       
       // student code here
        for (int i = 0; i < numColumns; i++) {
            System.out.print("#");
        }




       System.out.println();
       System.out.println("Task complete");
   }
} 
                
