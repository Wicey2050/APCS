import java.util.*;
class Even
{
   public static void main(String[] args)
   {
       Scanner input = new Scanner(System.in);
       System.out.print("Enter an integer: " );
       int target = input.nextInt();
       input.close();
       
       // student code here
        for (int i = 2; i <= target; i += 2) {
            if ((i % 2) == 0 && i <= target) {
                System.out.print(i + " ");
            }
        }

   }
} 
                
